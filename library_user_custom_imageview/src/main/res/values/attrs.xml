<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="UserCustomImageView">
        <attr name="piv_valueOfSpeedAnimation" format="integer"/>
        <attr name="piv_borderColor" format="color"/>
        <attr name="piv_borderWidth" format="dimension"/>
        <attr name="piv_initial" format="string"/>
        <attr name="piv_isAnimated" format="boolean"/>
        <attr name="piv_valueOfIncrease" format="integer"/>
        <attr name="piv_animationInterpolator" format="enum">
            <enum name="AccelerateDecelerateInterpolator" value="0"/>
            <enum name="AccelerateInterpolator" value="1"/>
            <enum name="DecelerateInterpolator" value="2"/>
            <enum name="LinearInterpolator" value="3"/>
        </attr>
    </declare-styleable>

</resources>